def is_prime(num):
    flag=0
    for i in range(2,int((num/2)+1)):
        if(num%i==0):
            flag=1
            break
    if(flag==0):
        return num

for i in range(1, 20):
    if is_prime(i + 1):
        print(i + 1, end=" ")
print()

def is_prime(num):
    for i in range(2, int(1 + num ** 0.5)):
        if num % i == 0:
            return False
    return True

for i in range(1, 20):
    if is_prime(i + 1):
        print(i + 1, end=" ")
print()

